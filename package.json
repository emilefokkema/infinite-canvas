{
  "name": "ef-infinite-canvas",
  "version": "0.6.0-alpha",
  "description": "An infinite version of the html canvas",
  "main": "dist/infinite-canvas.js",
  "types": "dist/types/infinite-canvas.d.ts",
  "private": false,
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --config=./test/jest.config.js",
    "copy-e2e": "node ./copy-e2e.js",
    "build-e2e-test-page": "npm run build-test-page-lib && npm run copy-e2e",
    "build-test-page-lib": "webpack --config=test-page.webpack.config.js",
    "test-e2e": "npm run build-e2e-test-page && jest --config=./test/e2e/jest-default.config.js",
    "test-e2e-gitpod": "npm run build-e2e-test-page && jest --config=./test/e2e/jest-gitpod.config.js",
    "start-e2e-server": "npm run build-e2e-test-page && node ./test/e2e/server/start.js",
    "build": "webpack --config=./webpack-build.config.js",
    "build:types": "npx tsc --project ts-config-types.json",
    "update-snapshot": "jest --updateSnapshot",
    "dev": "node ./docs/dev/start.js",
    "typedoc": "typedoc"
  },
  "keywords": [
    "canvas",
    "infinite"
  ],
  "homepage": "https://github.com/emilefokkema/infinite-canvas",
  "bugs": {
    "url": "https://github.com/emilefokkema/infinite-canvas/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emilefokkema/infinite-canvas.git"
  },
  "author": "Emile Fokkema",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@types/node": "^12.12.24",
    "@types/puppeteer": "^5.4.6",
    "express": "^4.18.2",
    "http-server": "^14.1.1",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-image-snapshot": "^6.1.0",
    "puppeteer": "^19.2.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.1",
    "typedoc": "^0.23.21",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "4.6.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.0",
    "ws": "^8.10.0"
  }
}
